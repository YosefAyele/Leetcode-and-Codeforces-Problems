#User function Template for python3

class Solution:
            
    def selectionSort(self, arr,n):
        #code here
        for i in range(n-1):
            minn=i
            for j in range(i+1,n):
                if arr[minn]>arr[j]:
                    minn=j
            if minn!=i:
                arr[minn],arr[i]=arr[i],arr[minn]
        return arr
                
